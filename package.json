{
  "name": "calories-timer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.12",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.1.1",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "concurrently \"yarn watch:css\" \"react-scripts start\"",
    "build": "yarn build:css && react-scripts build",
    "test": "react-scripts test",
    "build:css": "postcss src/css/tailwind/index.css -o src/css/tailwind.build.css",
    "watch:css": "postcss src/css/tailwind/index.css -o src/css/tailwind.build.css --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.0.2"
  }
}
